{
  "name": "@repo/security",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./jsonwebtoken": {
      "import": "./dist/jsonwebtoken/index.js",
      "types": "./dist/jsonwebtoken/index.d.ts"
    },
    "./hashing": {
      "import": "./dist/hashing-service/index.js",
      "types": "./dist/hashing-service/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "build": "pnpm run clean && tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist .turbo tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "google-auth-library": "^10.4.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
}